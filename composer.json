{
  "name": "templar1/silex-bootstrap",
  "description": "Bootstrap for Silex based projects",
  "type": "project",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Marek Sokół",
      "email": "marek@codegroup.pl"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "silex/silex": "~2.0",
    "monolog/monolog": "^1.20",
    "ivoba/dotenv-service-provider": "^2.0",
    "doctrine/dbal": "~2.2",
    "dflydev/doctrine-orm-service-provider": "^2.0",
    "doctrine/migrations": "^1.4",
    "knplabs/console-service-provider": "^2.0",
    "symfony/validator": "^3.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5"
  },
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "test"
    }
  }
}
