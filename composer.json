{
  "name": "templar1/silex-bootstrap",
  "description": "Bootstrap for Silex based API",
  "type": "project",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Marek Sokół",
      "email": "marek@codegroup.pl"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "silex/silex": "~2.0",
    "monolog/monolog": "^1.23",
    "doctrine/dbal": "~2.6",
    "dflydev/doctrine-orm-service-provider": "^2.0",
    "doctrine/migrations": "^1.5",
    "knplabs/console-service-provider": "^2.1",
    "symfony/validator": "^3.3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.3",
    "friendsofphp/php-cs-fixer": "^2.4"
  },
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "test"
    }
  }
}
